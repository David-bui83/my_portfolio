@{
    ViewData["Title"] = "No Page".ToUpper();
}
<partial name="Nav"/>

<section class="section-privacy mt-5">
    <div class="container mt-5">
        <h1 class="display-4 text-primary text-center">@ViewData["Title"]</h1>
        <span class="mb-5" style="margin: auto; width: 20%; border: 2px solid #abb6c2; display: block;"></span>
        <p class="text-center">The page you are looking for does not exist.</p>
        <div class="text-center mt-3">
          <a class="btn btn-primary" asp-action="Index" asp-controller="Home"><i class="fas fa-arrow-left"></i> Back to Home</a>
        </div>
    </div>
</section>
<partial name="footer"/>